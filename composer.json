{
    "name": "protonemedia/laravel-analytics-event-tracking",
    "description": "Laravel package to easily send events to Google Analytics",
    "keywords": [
        "protonemedia",
        "laravel",
        "google-analytics",
        "laravel-analytics",
        "analytics",
        "laravel-analytics-event-tracking",
        "analytics-event-tracking"
    ],
    "homepage": "https://github.com/protonemedia/laravel-analytics-event-tracking",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/bus": "^10.0",
        "illuminate/queue": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/validation": "^10.0",
        "theiconic/php-ga-measurement-protocol": "^2.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.4.4",
        "nesbot/carbon": "^2.66",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\AnalyticsEventTracking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\AnalyticsEventTracking\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\AnalyticsEventTracking\\ServiceProvider"
            ]
        }
    }
}
